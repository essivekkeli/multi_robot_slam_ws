    </model>

    <model name="cylinder2">
      <pose>0 6 0.35 0 0 0</pose>
      <static>true</static>
      <link name="link">
        <collision name="collision">
          <geometry>
            <cylinder><radius>0.25</radius><length>0.7</length></cylinder>
          </geometry>
        </collision>
        <visual name="visual">
          <geometry>
            <cylinder><radius>0.25</radius><length>0.7</length></cylinder>
          </geometry>
          <material>
            <ambient>0.9 0.7 0.2 1</ambient>
            <diffuse>0.9 0.7 0.2 1</diffuse>
