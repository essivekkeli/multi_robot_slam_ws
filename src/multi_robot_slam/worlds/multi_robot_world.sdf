    </model>

    <model name="box4">
      <pose>7 0 0.3 0 0 0.3</pose>
      <static>true</static>
      <link name="link">
        <collision name="collision">
          <geometry>
            <box><size>0.5 1.5 0.6</size></box>
          </geometry>
        </collision>
        <visual name="visual">
          <geometry>
            <box><size>0.5 1.5 0.6</size></box>
          </geometry>
          <material>
            <ambient>0.3 0.7 0.5 1</ambient>
            <diffuse>0.3 0.7 0.5 1</diffuse>
          </material>
        </visual>
      </link>
    </model>



  </world>
</sdf>