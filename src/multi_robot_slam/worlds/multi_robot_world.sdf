    </model>

    <model name="box3">
      <pose>-6 -5 0.3 0 0 1.2</pose>
      <static>true</static>
      <link name="link">
        <collision name="collision">
          <geometry>
            <box><size>0.8 1.0 0.6</size></box>
          </geometry>
        </collision>
        <visual name="visual">
          <geometry>
            <box><size>0.8 1.0 0.6</size></box>
          </geometry>
          <material>
