    </model>

    <model name="cylinder1">
      <pose>6 -4 0.4 0 0 0</pose>
      <static>true</static>
      <link name="link">
        <collision name="collision">
          <geometry>
            <cylinder><radius>0.3</radius><length>0.8</length></cylinder>
